<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PantallaInicial.HomeFragment">

    <TextView
        android:id="@+id/text_view_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="50dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="50dp"
        android:text="Tiempo dedicado a ejercicios:" />

    <TextView
        android:id="@+id/text_view_caloryCount"
        android:layout_width="58dp"
        android:layout_height="23dp"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="300dp" />

    <TextView
        android:id="@+id/text_view_calory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="300dp"
        android:text="CalorÃ­as quemadas:" />

    <TextView
        android:id="@+id/text_view_timeCount"
        android:layout_width="58dp"
        android:layout_height="23dp"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="50dp" />

    <TextView
        android:id="@+id/text_view_id"
        android:layout_width="140dp"
        android:layout_height="18dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="600dp"
        android:text="Numero de ejercicios completados: " />

    <TextView
        android:id="@+id/text_view_idCount"
        android:layout_width="58dp"
        android:layout_height="23dp"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="600dp" />

</FrameLayout>